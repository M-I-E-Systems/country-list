{
    "name": "MIESystems/country-list",
    "description": "List of all countries with names and ISO 3166-1 codes in all languages and data formats for Laravel",
    "keywords": ["laravel", "countries"],
    "license": "GNU",
    "authors": [
        {
            "name": "Momen Noor",
            "email": "momen.noor@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "illuminate/support": "^5.8|^6|^7|^8",
        "umpirsky/country-list": "2.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3"
    },
    "autoload": {
        "psr-4": {
            "MIESystems\\CountryList\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MIESystems\\CountryList\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "MIESystems\\CountryList\\CountryListServiceProvider"
            ],
            "aliases": {
                "Countries": "MIESystems\\CountryList\\CountryListFacade"
            }
        }
    }
}
